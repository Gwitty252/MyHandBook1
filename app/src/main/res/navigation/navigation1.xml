<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation1"
    app:startDestination="@id/splash1">
    <fragment
        android:id="@+id/splash1"
        android:name="io.gideon.myhandbook1.Splash1"
        android:label="fragment_splash1"
        tools:layout="@layout/fragment_splash1" >
        <action
            android:id="@+id/action_splash1_to_myViewPager"
            app:destination="@id/myViewPager" />
    </fragment>
    <fragment
        android:id="@+id/myViewPager"
        android:name="io.gideon.myhandbook1.MyViewPager"
        android:label="fragment_my_view_pager"
        tools:layout="@layout/fragment_my_view_pager" >
        <action
            android:id="@+id/action_myViewPager_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_myViewPager_to_homeFragment2"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_myViewPager_to_homeFragment3"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_myViewPager_to_homeFragment4"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="io.gideon.myhandbook1.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
</navigation>